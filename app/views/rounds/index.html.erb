<div class="container">
<h1>Listing rounds</h1>

<table class="table table-hover table-responsive table-bordered">
  <thead>
    <tr>
      <th>Id</th>
      <th>Identifier</th>
      <th>Tee_id</th>
      <th>Player_id</th>
      <th>Course_id</th>
      <th>Starthole</th>
      <th>Endhole</th>
      <th>Score</th>
    </tr>
  </thead>

  <tbody>
    <% @rounds.each do |round| %>
      <tr>
        <td><%= round.id %></td>
        <td><%= round.identifier %></td>
        <td><%= round.tee_id %></td>
        <td><%= round.player_id %></td>
        <td><%= round.course_id %></td>
        <td><%= round.startHole %></td>
        <td><%= round.endHole %></td>
        <td><%= round.score %></td>
        <td><%= link_to 'Show', round %></td>
        <td><%= link_to 'Edit', edit_round_path(round) %></td>
        <td><%= link_to 'Destroy', round, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Round', new_round_path, :class=>"btn btn-primary" %>
<%= link_to '.CSV', rounds_path(format: "csv") , :class=>"btn btn-primary" %>
</div>