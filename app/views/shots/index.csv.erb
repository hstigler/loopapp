<%=
response.content_type = 'application/shots'
CSV.generate do |csv|
	csv << ["ID", "IDENTIFIER", "CLUB_ID", "ROUND_HOLE_ID", "STARTLAT", "STARTLON", "ENDLAT", "ENDLON", "SCORE", "LIE", "TIME"]
	@shots.each do |shot|
    csv << [ shot[:id], shot[:identifier], shot[:club_id], shot[:round_hole_id], shot[:startlat], shot[:startlon], shot[:endlat], shot[:endlon], shot[:score], shot[:lie], shot[:time] ]
  end
end .html_safe
%>