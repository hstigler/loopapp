<div class="container">
<h1>Listing tees</h1>

<table class="table table-hover table-responsive table-bordered">
  <thead>
    <tr>
      <th>Id</th>
      <th>Identifier</th>
      <th>Name</th>
      <th>Distance</th>
      <th>Courserating</th>
      <th>Sloperating</th>
    </tr>
  </thead>

  <tbody>
    <% @tees.each do |tee| %>
      <tr>
        <td><%= tee.id %></td>
        <td><%= tee.identifier %></td>
        <td><%= tee.name %></td>
        <td><%= tee.distance %></td>
        <td><%= tee.courseRating %></td>
        <td><%= tee.slopeRating %></td>
        <td><%= link_to 'Show', tee %></td>
        <td><%= link_to 'Edit', edit_tee_path(tee) %></td>
        <td><%= link_to 'Destroy', tee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tee', new_tee_path, :class=>"btn btn-primary" %>
<%= link_to '.CSV', tees_path(format: "csv") , :class=>"btn btn-primary" %>
</div>